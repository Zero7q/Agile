@model Agile.Models.ViewModels.SysDictionaryDetailViewModel
@{
    ViewData["Title"] = "Add";
    Layout = "~/Views/Shared/_EditLayout.cshtml";
}
@Html.HiddenFor(s => s.Id)
@Html.HiddenFor(s => s.CreateTime)
@Html.HiddenFor(s => s.UpdateTime)
@Html.HiddenFor(s => s.DictionaryId)
<div class="layui-row">
    <div class="layui-col-sm12">
        <div class="layui-form-item">
            <label class="layui-form-label">@Html.DisplayNameFor(s => s.DictionaryKey)：</label>
            <div class="layui-input-block">
                @Html.TextBoxFor(s => s.DictionaryKey, new { @class = "layui-input", @placeholder = $"请输入{Html.DisplayNameFor(s => s.DictionaryKey)}" })
            </div>
        </div>
        <div class="layui-form-item">
            <label class="layui-form-label">@Html.DisplayNameFor(s => s.DictionaryValue)：</label>
            <div class="layui-input-block">
                @Html.TextBoxFor(s => s.DictionaryValue, new { @class = "layui-input", @placeholder = $"请输入{Html.DisplayNameFor(s => s.DictionaryValue)}" })
            </div>
        </div>
    </div>
</div>
